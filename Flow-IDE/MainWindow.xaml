<Window x:Class="Flow_IDE.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Flow_IDE"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525">

    <DockPanel x:Name="MainDockPanel" LastChildFill="True">
        <StackPanel x:Name="LeftStackPanel" DockPanel.Dock="Left"/>
        <StackPanel x:Name="RightStackPanel" DockPanel.Dock="Right"/>
        <StackPanel x:Name="TopStackPanel" DockPanel.Dock="Top"/>
        <StackPanel x:Name="BottomStackPanel" DockPanel.Dock="Bottom"/>
    </DockPanel>
</Window>
